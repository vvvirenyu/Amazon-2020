    "def fillTheTruck(num: int, boxes: List[int], unitSize: int, unitsPerBox: List[int], truckSize: int) -> int:  \n",
    "    \n",
    "    units = []\n",
    "    for box, upb in zip(boxes, unitsPerBox):\n",
    "        units += [upb] * box\n",
    "    return sum(sorted(units, reverse=True)[:truckSize])"
   ]
  }
